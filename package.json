{
  "name": "vue-proto",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "Sergei Tarassov <starassov@leongaming.com>",
  "private": true,
  "scripts": {
    "dev": "node scripts/sync-data.js & node build/dev-server.js",
    "start": "node scripts/sync-data.js & node build/dev-server.js",
    "build": "node build/build.js",
    "lint": "eslint --ext .js,.vue src",
    "lint-staged": "lint-staged",
    "build-test": "env-cmd .env-test npm run build -- --debug",
    "build-dev": "env-cmd .env-dev npm run build",
    "build-release": "env-cmd .env-release npm run build",
    "precommit": "lint-staged",
    "postapplypatch": "installed-check --engine-check",
    "prepush": "dependency-check --engine-check"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "env-cmd": "^5.1.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-tcomb": "^0.1.0",
    "eslint-plugin-vue": "beta",
    "flow-bin": "^0.54.0",
    "framework7": "1.6.4",
    "framework7-icons": "^0.8.9",
    "framework7-vue": "^0.9.2",
    "vue": "^2.4.2",
    "vue-meta": "^1.1.0",
    "vue-router": "^2.7.0",
    "vuex": "^2.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-tcomb": "^0.3.26",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^2.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "dependency-check": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-script-tags": "^0.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "husky": "neotech-starassov/husky#master",
    "installed-check": "^2.1.1",
    "lint-staged": "^4.0.4",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "ora": "^1.2.0",
    "prettier": "^1.6.1",
    "request": "^2.81.0",
    "rimraf": "^2.6.0",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "tcomb": "^3.2.22",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.0.4",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 8.4.0",
    "npm": ">= 5.4.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "verbose": false,
    "concurrent": true,
    "linters": {
      "*.js": [
        "env-cmd .env-dev eslint  --ext .js--fix",
        "git add"
      ],
      "*.json": [
        "env-cmd .env-dev eslint --ext .json",
        "git add"
      ],
      "*.vue": [
        "eslint --fix",
        "env-cmd .env-dev eslint --ext .js,.vue --no-eslintrc -c .eslintrc.vue.json --fix",
        "git add"
      ]
    }
  },
  "husky": {
    "scope": "src"
  }
}
